{
    "name": "daikazu/laravel-glider",
    "description": "Start using Glide on-the-fly instantly in your Laravel blade templates.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Wall",
            "email": "daikazu@gmail.com",
            "homepage": "http://mikewall.dev"
        }
    ],
    "homepage": "https://github.com/daikazu/laravel-glider",
    "keywords": [
        "Laravel",
        "Laravel Glider",
        "Glider",
        "blade",
        "glide"
    ],
    "require": {
        "illuminate/support": "~8|~9|~10",
        "league/glide-laravel": "^1.0",
        "league/flysystem": "^3.0",
        "intervention/image": "^2.7",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.4"
    },
    "autoload": {
        "psr-4": {
            "Daikazu\\LaravelGlider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daikazu\\LaravelGlider\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daikazu\\LaravelGlider\\LaravelGliderServiceProvider"
            ],
            "aliases": {
                "Glider": "Daikazu\\LaravelGlider\\Facades\\LaravelGlider"
            }
        }
    }
}
